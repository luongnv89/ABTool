SYSTEM
	toy

VARIABLES
	xx,yy

INVARIANT
	xx,yy : NAT * NAT

INITIALISATION
	xx,yy := 0,0

EVENTS
	evt_xx 	= 	xx := xx +1;
	evtyy	=	yy := yy +1

END
